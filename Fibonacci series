
package fibonacci.series;
import java.util.*;

public class FibonacciSeries {

    
    public static void main(String[] args) {
        Scanner sc =new Scanner(System.in);
        System.out.println("Enter number of terms");
        int num=sc.nextInt();
        int a=0;
        int b=1;
        System.out.print(a+" "+b+" ");
        for(int i=2;i<num;i++){
            int c=a+b;
            System.out.print(c+" ");
            a=b;
            b=c;
            
        }
        
        
    }
    
}
